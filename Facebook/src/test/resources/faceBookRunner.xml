<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Facebook Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Majove"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="73.0"/>
    <parameter name="url" value="https://www.facebook.com"/>

    <test name = "Facebook Runner">
        <classes>
            <class name="TestFaceBook"/>
            <class name="TestFBPage"/>
            <class name="TestFBSheets"/>
        </classes>
    </test>

</suite>
